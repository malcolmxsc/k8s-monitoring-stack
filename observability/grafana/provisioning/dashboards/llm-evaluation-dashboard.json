      "5m"
    ]
  },
  "templating": {
    "list": [
      {
        "name": "interval",
        "label": "Interval",
        "type": "interval",
        "auto": true,
        "auto_count": 30,
        "auto_min": "10s",
        "query": "10s,30s,1m,5m,15m",
