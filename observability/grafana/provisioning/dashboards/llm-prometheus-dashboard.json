{
  "title": "LLM Model Reliability (Prometheus)",
  "tags": ["llm", "errors", "prometheus", "reliability"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "title": "Model Error Rates (%)",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 6
      },
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(llm_errors_total[5m])) by (model) / (sum(rate(llm_prompts_total[5m])) by (model) + sum(rate(llm_errors_total[5m])) by (model)) * 100",
          "refId": "A",
          "legendFormat": "{{model}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "orange"
              },
              {
                "value": 15,
                "color": "red"
              }
            ]
          },
          "mappings": []
        }
      },
      "options": {
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center"
      }
    },
    {
      "title": "Total Errors by Model (5m rate)",
      "type": "barchart",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 8
      },
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(llm_errors_total[5m])) by (model) * 300",
          "refId": "A",
          "legendFormat": "{{model}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short",
          "decimals": 1
        }
      },
      "options": {
        "orientation": "vertical",
        "barWidth": 0.8,
        "showValue": "always",
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "values": ["value"]
        }
      }
    },
    {
      "title": "Errors by Type",
      "type": "piechart",
      "gridPos": {
        "x": 12,
        "y": 6,
        "w": 12,
        "h": 8
      },
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(llm_errors_total) by (error_type)",
          "refId": "A",
          "legendFormat": "{{error_type}}"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "values": ["value", "percent"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "title": "Error Rate Over Time by Model",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 24,
        "h": 8
      },
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "rate(llm_errors_total[1m]) * 60",
          "refId": "A",
          "legendFormat": "{{model}} - {{error_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "never",
            "spanNulls": true
          },
          "unit": "cpm",
          "decimals": 1
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["mean", "max", "last"]
        }
      }
    },
    {
      "title": "Request Success Rate",
      "type": "gauge",
      "gridPos": {
        "x": 0,
        "y": 22,
        "w": 12,
        "h": 6
      },
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(llm_prompts_total[5m])) / (sum(rate(llm_prompts_total[5m])) + sum(rate(llm_errors_total[5m]))) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 80,
                "color": "orange"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "orientation": "auto",
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "Total Requests (5m rate)",
      "type": "stat",
      "gridPos": {
        "x": 12,
        "y": 22,
        "w": 12,
        "h": 6
      },
      "id": 6,
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(llm_prompts_total[5m])) * 300 + sum(rate(llm_errors_total[5m])) * 300",
          "refId": "A",
          "legendFormat": "Total Requests"
        },
        {
          "expr": "sum(rate(llm_prompts_total[5m])) * 300",
          "refId": "B",
          "legendFormat": "Successful"
        },
        {
          "expr": "sum(rate(llm_errors_total[5m])) * 300",
          "refId": "C",
          "legendFormat": "Failed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Failed"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Successful"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "title": "Recent Error Logs",
      "type": "logs",
      "gridPos": {
        "x": 0,
        "y": 28,
        "w": 24,
        "h": 8
      },
      "id": 7,
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "expr": "{job=\"los-app\"} |= \"generate_error\" | json",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "templating": {
    "list": []
  }
}
