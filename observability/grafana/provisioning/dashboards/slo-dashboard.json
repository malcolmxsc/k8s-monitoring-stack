{
  "title": "SLO Dashboard",
  "tags": ["slo", "observability"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
      {
        "title": "Request Latency (P90 & P95)",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "id": 1,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "histogram_quantile(0.90, sum(rate(http_server_requests_seconds_bucket{uri=\"/generate\"}[1m])) by (le))",
            "legendFormat": "P90 Latency",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{uri=\"/generate\"}[1m])) by (le))",
            "legendFormat": "P95 Latency",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "showPoints": "auto",
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.5,
                  "color": "yellow"
                },
                {
                  "value": 1,
                  "color": "red"
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "title": "P90 Latency SLO",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "id": 2,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "histogram_quantile(0.90, sum(rate(http_server_requests_seconds_bucket{uri=\"/generate\"}[1m])) by (le))",
            "legendFormat": "P90 Latency (seconds)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.5,
                  "color": "yellow"
                },
                {
                  "value": 1.0,
                  "color": "red"
                }
              ]
            },
            "max": 2,
            "min": 0
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "title": "Availability SLO",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "id": 3,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "(1 - (sum(rate(http_server_requests_seconds_count{uri=\"/generate\",status=~\"5..\"}[5m])) / sum(rate(http_server_requests_seconds_count{uri=\"/generate\"}[5m])))) * 100",
            "legendFormat": "Availability",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 99.9,
                  "color": "green"
                }
              ]
            },
            "max": 100,
            "min": 99
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "title": "Error Rate",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 4
        },
        "id": 4,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "sum(rate(http_server_requests_seconds_count{uri=\"/generate\",status=~\"5..\"}[1m])) / sum(rate(http_server_requests_seconds_count{uri=\"/generate\"}[1m]))",
            "legendFormat": "Server Errors (5xx)",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "sum(rate(http_server_requests_seconds_count{uri=\"/generate\",status=~\"4..\"}[1m])) / sum(rate(http_server_requests_seconds_count{uri=\"/generate\"}[1m]))",
            "legendFormat": "Client Errors (4xx)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "showPoints": "auto",
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 0.001,
                  "color": "yellow"
                },
                {
                  "value": 0.01,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 6
        },
        "id": 5,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "sum(rate(http_server_requests_seconds_count{uri=\"/generate\"}[1m]))",
            "legendFormat": "Total Requests/sec",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "sum(rate(http_server_requests_seconds_count{uri=\"/generate\",status=~\"2..\"}[1m]))",
            "legendFormat": "Successful (2xx)",
            "refId": "B"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "sum(rate(http_server_requests_seconds_count{uri=\"/generate\",status=~\"5..\"}[1m]))",
            "legendFormat": "Errors (5xx)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "showPoints": "auto",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "title": "Active Alerts",
        "type": "table",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 6
        },
        "id": 6,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "ALERTS{alertstate=\"firing\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "Value": true,
                "__name__": true,
                "alertstate": true,
                "job": true,
                "instance": true,
                "error": true,
                "exception": true,
                "method": true,
                "outcome": true,
                "status": true,
                "uri": true
              },
              "indexByName": {
                "alertname": 0,
                "severity": 1,
                "slo": 2
              },
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "slo": "SLO Type"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "links": [
              {
                "title": "View Logs in Loki",
                "url": "/explore?left={\"datasource\":\"P8E80F9AEF21F6940\",\"queries\":[{\"refId\":\"A\",\"expr\":\"${__data.fields.alertname}\",\"datasource\":{\"type\":\"loki\",\"uid\":\"P8E80F9AEF21F6940\"}}],\"range\":{\"from\":\"now-1h\",\"to\":\"now\"}}"
              }
            ]
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Alert"
              },
              "properties": [
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "üîç View Error Logs (Last 15m)",
                      "url": "/explore?left={\"datasource\":\"P8E80F9AEF21F6940\",\"queries\":[{\"refId\":\"A\",\"expr\":\"{job=\\\"los-app\\\"} |= \\\"ERROR\\\"\",\"datasource\":{\"type\":\"loki\",\"uid\":\"P8E80F9AEF21F6940\"}}],\"range\":{\"from\":\"now-15m\",\"to\":\"now\"}}"
                    },
                    {
                      "title": "üîó View Traces with Errors",
                      "url": "/explore?left={\"datasource\":\"P214B5B846CF3925F\",\"queries\":[{\"refId\":\"A\",\"queryType\":\"traceql\",\"query\":\"{status=error}\",\"datasource\":{\"type\":\"tempo\",\"uid\":\"P214B5B846CF3925F\"}}],\"range\":{\"from\":\"now-15m\",\"to\":\"now\"}}"
                    },
                    {
                      "title": "üìä View Alert Metric in Prometheus",
                      "url": "/explore?left={\"datasource\":\"PBFA97CFB590B2093\",\"queries\":[{\"refId\":\"A\",\"expr\":\"ALERTS{alertname=\\\"${__data.fields.Alert}\\\"}\",\"datasource\":{\"type\":\"prometheus\",\"uid\":\"PBFA97CFB590B2093\"}}],\"range\":{\"from\":\"now-1h\",\"to\":\"now\"}}"
                    },
                    {
                      "title": "üö® View 5xx Error Logs (Last 30m)",
                      "url": "/explore?left={\"datasource\":\"P8E80F9AEF21F6940\",\"queries\":[{\"refId\":\"A\",\"expr\":\"{job=\\\"los-app\\\"} |~ \\\"status=5[0-9]{2}\\\"\",\"datasource\":{\"type\":\"loki\",\"uid\":\"P8E80F9AEF21F6940\"}}],\"range\":{\"from\":\"now-30m\",\"to\":\"now\"}}"
                    }
                  ]
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Severity"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "mappings",
                  "value": [
                    {
                      "type": "value",
                      "options": {
                        "critical": {
                          "color": "red"
                        }
                      }
                    },
                    {
                      "type": "value",
                      "options": {
                        "warning": {
                          "color": "yellow"
                        }
                      }
                    },
                    {
                      "type": "value",
                      "options": {
                        "info": {
                          "color": "blue"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      },
      {
        "title": "LLM Token Usage",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 14,
          "w": 12,
          "h": 6
        },
        "id": 7,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "rate(llm_request_tokens_sum[1m]) / rate(llm_request_tokens_count[1m])",
            "legendFormat": "Avg Request Tokens",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "rate(llm_response_tokens_sum[1m]) / rate(llm_response_tokens_count[1m])",
            "legendFormat": "Avg Response Tokens",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "showPoints": "auto",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "title": "Resource Utilization",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 14,
          "w": 12,
          "h": 6
        },
        "id": 8,
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "sum(jvm_memory_used_bytes{area=\"heap\"}) / sum(jvm_memory_max_bytes{area=\"heap\"}) * 100",
            "legendFormat": "Heap Memory Usage",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "jvm_threads_live_threads",
            "legendFormat": "Active Threads",
            "refId": "B"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            },
            "expr": "rate(jvm_gc_pause_seconds_sum[1m]) / rate(jvm_gc_pause_seconds_count[1m]) * 1000",
            "legendFormat": "Avg GC Pause Time",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "showPoints": "auto",
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Heap Memory Usage"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "max",
                  "value": 100
                },
                {
                  "id": "min",
                  "value": 0
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Avg GC Pause Time"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "ms"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Active Threads"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                }
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      }
    ]
}
